    <rules>
        <fullName>AplazamientoReclamacion</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Case.RecordTypeId</field>
            <operation>equals</operation>
            <value>Reclamación</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.CAS_DAT_Fecha_de_Aplazamiento__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>El día indicado por el Gestor en el campo &quot;Fecha de aplazamiento&quot; se genera una tarea a éste para indicáselo</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Reclamacion_Aplazada</name>
                <type>Task</type>
            </actions>
            <offsetFromField>Case.CAS_DAT_Fecha_de_Aplazamiento__c</offsetFromField>
            <timeLength>0</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Asignación Tramitación Siniestros</fullName>
        <actions>
            <name>Asignar_Cola_Siniestro</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Asigna una cola para que se asigne el gestor de tramitación específico para cada siniestro</description>
        <formula>ISCHANGED(Status) &amp;&amp; ISPICKVAL(Status,&quot;001&quot;) &amp;&amp; CAS_FOR_RTDevName__c = $Label.RT_CAS_SINIESTRO &amp;&amp; PRIORVALUE(Status) != $Label.CAS_ESTADO_ESPERA_GESTOR_CONFIRMACION &amp;&amp; PRIORVALUE(Status) != $Label.CAS_ESTADO_ESPERA_PROFESIONAL_CONFIRMACION</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>AvisoTareaFechaApertura</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Case.RecordTypeId</field>
            <operation>equals</operation>
