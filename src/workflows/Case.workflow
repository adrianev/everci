    <rules>
        <fullName>ActivaciónCasoPreventivo</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Case.CAS_DAT_Fecha_activacion__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Type</field>
            <operation>equals</operation>
            <value>98</value>
        </criteriaItems>
        <description>Cuando llega la fecha del campo &quot;Fecha activación&quot; el caso preventivo pasa de estado Inactivo a estaado en curso</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
