    <webLinks>
        <fullName>Renovar_Contrato</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Renovar Contrato</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/40.0/connection.js&quot;)}
{!REQUIRESCRIPT(&apos;//ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js&apos;)}
{!REQUIRESCRIPT(&apos;//ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js&apos;)}
debugger;
var version = {!Contract.CTR_NUM_Version_Contrato__c}; 
var date = &quot;{!Contract.StartDate}&quot;.split(&apos;/&apos;);
version = version +1;
var newContract = new sforce.SObject(&quot;Contract&quot;);
newContract.Name = &quot;{!Contract.Name}&quot;;
newContract.AccountId = &quot;{!Contract.AccountId}&quot;;
newContract.Status = &quot;Borrador&quot;;
newContract.StartDate = new Date(date[2],date[1]-1,date[0]);
newContract.ContractTerm = {!Contract.ContractTerm};
newContract.CTR_NUM_Version_Contrato__c = version;
newContract.CTR_LKP_Clonado_De__c = &quot;{!Contract.Id}&quot;;
var result = sforce.connection.create([newContract]);
try{
  jQuery(function() {
    jQuery(&apos;head&apos;).append(&apos;&lt;link rel=&quot;stylesheet&quot; href=&quot;//ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/themes/smoothness/jquery-ui.css&quot; type=&quot;text/css&quot; /&gt;&apos;);
    
    var html = 
      &apos;&lt;div id=&quot;dialog&quot; align=&quot;center&quot; title=&quot;Renovando Contrato&quot;&gt;&lt;p&gt;recuerde que tiene que modificar la fecha de inicio del contrato&lt;/p&gt;&lt;/div&gt;&apos;;

    if(!jQuery(&apos;[id=dialog]&apos;).size()){
      jQuery(&apos;body&apos;).append(html);
    }     
    jQuery( &quot;#dialog&quot; ).dialog({
      autoOpen: true,
      modal: true,
      show: {
        effect: &quot;Puff&quot;,
        duration: 1000
      },
      hide: {
        effect: &quot;Puff&quot;,
        duration: 1000
      },
      buttons: {
        &quot;Continuar&quot;: function() {
           window.location.href=&quot;/&quot;+result[0].id;
          jQuery( this ).dialog( &quot;close&quot; );
        }
      }
    });
  }); 
}
catch(e){
alert(&apos;´Ha ocurrido un error. Error: &apos; + e);
}</url>
    </webLinks>
