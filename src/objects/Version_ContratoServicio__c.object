        <formula>IF( POLV_LKP_PolizaContrato__r.POL_LKP_Compania__r.CUE_TXT_Identificador__c == $Label.IDE_SEGURCAIXA , 
IF( POLV_TXT_VIPCompany__c == &apos;A&apos; || POLV_TXT_VIPCompany__c == &apos;P&apos;|| POLV_TXT_VIPCompany__c == &apos;S&apos;,&apos;03&apos;, 
IF ( POLV_TXT_VIPCompany__c == &apos;B&apos; || POLV_TXT_VIPCompany__c == &apos;F&apos;,&apos;02&apos;, 
IF ( POLV_TXT_VIPCompany__c == &apos;N&apos;,&apos;01&apos;,&apos;01&apos;) 
) 
), 

IF( POLV_LKP_PolizaContrato__r.POL_LKP_Compania__r.CUE_TXT_Identificador__c == $Label.IDE_RGA, 
IF( POLV_CAS_IsGold__c == true , 
&apos;04&apos;, 
IF( POLV_LKP_PolizaContrato__r.POL_LKP_Compania__r.CUE_TXT_Identificador__c == $Label.IDE_NATIONALE_NETHERLANDEN, 
IF( POLV_LKP_PolizaContrato__r.POL_LKP_Producto_Cia__r.PRD_TXT_Cod_Producto__c == &apos;PH6&apos;, 
&apos;05&apos;, 
&apos;01&apos;) 
,&apos;01&apos;) 
) 
,&apos;01&apos;))</formula>
        <label>Segmentación (Código)</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
