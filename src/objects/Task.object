        <fullName>Aceptar</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Aceptar</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/40.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/40.0/apex.js&quot;)} 
debugger; 

// Recoger caso
var queryCaso = sforce.connection.query(&quot;SELECT Id, CAS_CAS_Informe_Preliminar_Recibido__c FROM Case WHERE Id = &apos;{!Task.What}&apos; LIMIT 1&quot;); 
var registros = queryCaso.getArray(&quot;records&quot;);
var caso = registros[0];
caso.CAS_CAS_Informe_Preliminar_Recibido__c = true;

var actCaso = sforce.connection.update([caso]); 
if (actCaso[0].success == &apos;true&apos;) { 
	var tareaActualizar = new sforce.SObject(&quot;Task&quot;); 
	tareaActualizar.Id = &quot;{!Task.Id}&quot;; 
	tareaActualizar.Status = &quot;Finalizado&quot;; 
	var actTarea = sforce.connection.update([tareaActualizar]);
	window.location.href = &quot;/&quot; + caso.Id; 
}else{			
	alert(actCaso[0].errors.message);
}</url>
    </webLinks>
