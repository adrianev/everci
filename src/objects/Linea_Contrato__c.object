    <validationRules>
        <fullName>Editar_campo_identificador</fullName>
        <active>true</active>
        <description>El campo identificador  no puede ser modificado</description>
        <errorConditionFormula>IF(AND(NOT(ISBLANK(PRIORVALUE(LDC_TXT_Identificador__c ))),ISCHANGED(LDC_TXT_Identificador__c )), true, false)</errorConditionFormula>
        <errorMessage>El campo identificador  no puede ser modificado</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LDC_RDV_GremiosUrgentes</fullName>
        <active>true</active>
        <description>Cuando una línea de contrato tenga el check de urgente marcado, sólo podrá seleccionar los gremios que permiten urgencias</description>
        <errorConditionFormula>(LDC_CAS_Urgente__c  = True) &amp;&amp; (NOT(ISPICKVAL(LDC_SEL_Gremio__c, (&apos;05&apos;)))) &amp;&amp; 
(LDC_CAS_Urgente__c  = True) &amp;&amp; (NOT(ISPICKVAL(LDC_SEL_Gremio__c, (&apos;01&apos;)))) &amp;&amp; 
(LDC_CAS_Urgente__c  = True) &amp;&amp; (NOT(ISPICKVAL(LDC_SEL_Gremio__c, (&apos;06&apos;)))) &amp;&amp; 
(LDC_CAS_Urgente__c  = True) &amp;&amp; (NOT(ISPICKVAL(LDC_SEL_Gremio__c, (&apos;07&apos;)))) &amp;&amp; 
(LDC_CAS_Urgente__c  = True) &amp;&amp; (NOT(ISPICKVAL(LDC_SEL_Gremio__c, (&apos;12&apos;)))) &amp;&amp; 
(LDC_CAS_Urgente__c  = True) &amp;&amp; (NOT(ISPICKVAL(LDC_SEL_Gremio__c, (&apos;&apos;))))</errorConditionFormula>
        <errorMessage>El gremio seleccionado no permite urgencias</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requerimiento_de_identificador</fullName>
        <active>false</active>
        <description>Regla de validación para requerir el campo Identificador</description>
        <errorConditionFormula>OR(ISBLANK(LDC_TXT_Identificador__c),ISNULL( LDC_TXT_Identificador__c))</errorConditionFormula>
        <errorDisplayField>LDC_TXT_Identificador__c</errorDisplayField>
        <errorMessage>El campo Identificador es requerido</errorMessage>
    </validationRules>
