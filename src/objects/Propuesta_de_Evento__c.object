    <validationRules>
        <fullName>Cobro_Imputacion</fullName>
        <active>true</active>
        <errorConditionFormula>!$Setup.Baipas__c.Saltar_Regla_Validacion__c

&amp;&amp;

ISPICKVAL(EV_AB_SEL_Tipo_de_Evento__c , &apos;Cobro&apos;)

&amp;&amp;

(

(

 ISBLANK(EV_AB_LKP_Imputar_a_gestor__c)  &amp;&amp; 

 ISBLANK(EV_AB_LKP_Imputar_a_profesional__c) &amp;&amp; 

 ISBLANK(EV_DIV_Importe_a_imputar_Departamento__c )

)

||

NOT( ISBLANK( EV_AB_LKP_Liquidar_a_prof__c ))

)</errorConditionFormula>
        <errorMessage>El evento es de cobro, debe rellenar los campos correspondientes a imputación. Los campos de liquidación no deben estar rellenos.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Gestor_Ev_estado_Nuevo</fullName>
        <active>true</active>
        <description>El gestor de reclamaciones solamente puede crear Eventos manuales en estado &quot;Nuevo&quot;.</description>
        <errorConditionFormula>AND($Profile.Name = &quot;Gestor Reclamaciones&quot;,
     EV_AB_CAS_Evento_Automatico__c == false,
     NOT(ISPICKVAL(EV_AB_SEL_Estado__c,&quot;04&quot;)),
      ISNEW()
         
)</errorConditionFormula>
        <errorMessage>El evento solo se puede crear en estado &quot;Nuevo&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Importes_incorrectos</fullName>
        <active>true</active>
        <errorConditionFormula>!$Setup.Baipas__c.Saltar_Regla_Validacion__c

&amp;&amp;

CASE( TEXT(EV_AB_SEL_Descripcion__c) ,
 &apos;E19&apos;,  IF(AND(EV_AB_DIV_Importe_profesional__c &lt;&gt; 10,
             EV_AB_DIV_Importe_profesional__c &lt;&gt; 40,
             EV_AB_DIV_Importe_profesional__c &lt;&gt; 90),1,0),
 &apos;E02&apos;, IF(EV_AB_DIV_Importe_a_liquidar_prof__c &lt;&gt; 90,1,0),
 &apos;E25&apos;, IF(EV_AB_DIV_Importe_profesional__c &lt;&gt; 15,1,0),
 &apos;E26&apos;, IF(EV_AB_DIV_Importe_a_liquidar_prof__c &lt;&gt; 15,1,0),
 &apos;E37&apos;, IF(EV_AB_DIV_Importe_a_liquidar_prof__c &lt;&gt; 25,1,0),
 &apos;E30&apos;, IF(EV_AB_DIV_Importe_profesional__c &lt;&gt; 30,1,0),
 &apos;E31&apos;, IF(EV_AB_DIV_Importe_profesional__c &lt;&gt; 25,1,0),
 &apos;E32&apos;, IF(EV_AB_DIV_Importe_profesional__c &lt;&gt; 15,1,0),
 &apos;E33&apos;, IF(EV_AB_DIV_Importe_profesional__c &lt;&gt; 20,1,0),
 &apos;E34&apos;, IF(EV_AB_DIV_Importe_a_liquidar_prof__c &lt;&gt; 25,1,0),
 0) = 1</errorConditionFormula>
        <errorMessage>El importe introducido es incorrecto para este código de evento.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_se_puede_modificar_evento_liquidado</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISCHANGED(EV_AB_CAS_Evento_Ab_Facturado__c)), EV_AB_CAS_Evento_Ab_Facturado__c,NOT(ISNEW()),
!$Setup.Baipas__c.Saltar_Regla_Validacion__c)</errorConditionFormula>
        <errorMessage>No se pueden modificar eventos que ya estén liquidados</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pago_Liquidacion</fullName>
        <active>true</active>
        <errorConditionFormula>!$Setup.Baipas__c.Saltar_Regla_Validacion__c

&amp;&amp;

ISPICKVAL(EV_AB_SEL_Tipo_de_Evento__c , &apos;Pago&apos;)

&amp;&amp;
(
(

 NOT(ISBLANK(EV_AB_LKP_Imputar_a_gestor__c))  ||

 NOT(ISBLANK(EV_AB_LKP_Imputar_a_profesional__c)) ||

 NOT(ISBLANK(EV_DIV_Importe_a_imputar_Departamento__c))

)

||

ISBLANK( EV_AB_LKP_Liquidar_a_prof__c )
)</errorConditionFormula>
        <errorMessage>El evento es de pago, debe rellenar los campos correspondientes a liquidación. Los campos de  imputación no deben estar rellenos.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rellenar_Observaciones_Evento_Rechazado</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(EV_AB_SEL_Estado__c ,&quot;03&quot;)&amp;&amp; (EV_AB_TXT_Observaciones__c = &apos;&apos;)</errorConditionFormula>
        <errorMessage>Si se rechaza la propuesta de Evento hay que indicar el motivo en el campo &apos;Observaciones&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rellenar_registro_asociado</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK( EV_AB_LKP_Cuenta_profesional__c ),ISBLANK( EV_AB_LKP_Caso__c ),ISBLANK( EV_AB_LKP_Orden_de_Trabajo__c ))</errorConditionFormula>
        <errorMessage>Es necesario asociar al evento un servicio, una cuenta o una intervención.</errorMessage>
    </validationRules>
