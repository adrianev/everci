    <webLinks>
        <fullName>Nueva_propuesta_de_evento</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Nueva propuesta de evento</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/xdomain/xdomain.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/26.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/support/console/30.0/integration.js&quot;)} 

var idObjeto = &quot;{!WorkOrder.Id}&quot;;

var url = &apos;&apos;;

if(idObjeto != &apos;&apos;){
url = &apos;/a17/e?CF00N9E000001iIzy={!WorkOrder.WorkOrderNumber}&amp;CF00N9E000001iIzy_lkid={!WorkOrder.Id}&amp;CF00N9E000001iJ0N={!WorkOrder.Case}&amp;CF00N9E000001iJ0N_lkid={!WorkOrder.CaseId}&amp;retURL=/{!WorkOrder.Id}&apos;;

} else {
idObjeto = &quot;{!Case.Id}&quot;;
if(idObjeto != &apos;&apos;){
url = &apos;/a17/e?CF00N9E000001iJ0N={!Case.CaseNumber}&amp;CF00N9E000001iJ0N_lkid={!Case.Id}&amp;retURL=/{!Case.Id}&apos;;
} else {
alert(&apos;ERROR: Este botón no está diseñado para ser utilizado aquí&apos;);
}

}

if(url != &apos;&apos;){

if (sforce.console.isInConsole()) { 
srcUp(url+&apos;&amp;isdtp=vw&apos;);
} else {
window.location = url;
}

}</url>
    </webLinks>
    <webLinks>
        <fullName>enviar_a_flujo_de_aprobacion</fullName>
        <availability>online</availability>
        <description>cambia el estado del evento a aprobado</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>enviar a flujo de aprobacion &gt;&gt;</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/42.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/42.0/apex.js&quot;)}
debugger;

var eventoHijo = &quot;{!Propuesta_de_Evento__c.EV_AB_LKP_Propuesta_de_Evento_Economico__c}&quot;;

if (eventoHijo == null  || eventoHijo == &apos;&apos;){
  var url = new URL(window.location.href); 
  var eventoId = url.pathname;

  eventoId= eventoId.substring(1,eventoId.length);

  var isUpdate = sforce.apex.execute(&quot;Util_Abonos_Y_Eventos&quot;,&quot;cambiarEstadoEvento&quot;, 
  {eventoId:eventoId});

  if (isUpdate == &apos;true&apos;) {
     document.location.reload(true);
     alert(&apos;Se ha enviado al proceso de aprobación con éxito&apos;);
     }else if(isUpdate == &apos;false&apos;){
           alert(&apos;El estado del evento es distinto a nuevo&apos;);
     }else{
         alert(&quot;{!$Label.ERROR_PERMISOS_USUARIO}&quot;);
      }


}
else {
  alert(&quot;{!$Label.WARNING_EVENTO_PADRE}&quot;);
}</url>
    </webLinks>
