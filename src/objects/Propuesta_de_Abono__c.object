    <validationRules>
        <fullName>Cuenta_a_ingresar_es_cuenta_del_caso</fullName>
        <active>true</active>
        <description>Comprobar que la cuenta a ingresar coincide con la cuenta o la compañía del caso pade del abono</description>
        <errorConditionFormula>(AB_LKP_Ingresar_a__c != AB_LKP_Caso__r.CAS_LKP_Compania__c) &amp;&amp; (AB_LKP_Ingresar_a__c != AB_LKP_Caso__r.AccountId)</errorConditionFormula>
        <errorMessage>El campo &quot;Ingresa a (CIA/Cliente)&quot; debe rellenarse con la compañía o la cuenta de la intervención asociada.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Gestor_Ab_estado_Nuevo</fullName>
        <active>true</active>
        <description>El gestor de reclamaciones solamente puede crear Abonos manuales en estado &quot;Nuevo&quot;.</description>
        <errorConditionFormula>AND($Profile.Name = &quot;Gestor Reclamaciones&quot;, NOT(ISPICKVAL(AB_SEL_Estado__c,&quot;04&quot;)),
 ISNEW())</errorConditionFormula>
        <errorMessage>El abono solo se puede crear en estado &quot;Nuevo&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ingresar_a_mayor_que_total</fullName>
        <active>true</active>
        <description>‘Importe ingresar a’ debe ser menor o igual al campo del servicio/intervención asociado</description>
        <errorConditionFormula>IF(NOT(ISBLANK(AB_LKP_Orden_de_Trabajo__c)), 
IF(OR(AB_LKP_Ingresar_a__r.CUE_FOR_RTDevName__c == &apos;Account:CUE_Compania&apos;,AB_LKP_Ingresar_a__r.CUE_FOR_RTDevName__c == &apos;Account:CUE_Cliente_B2C_ppal&apos;,AB_LKP_Ingresar_a__r.CUE_FOR_RTDevName__c == &apos;Account:CUE_Cliente_B2B_B2B2C_ppal&apos;), 
AB_LKP_Orden_de_Trabajo__r.OTR_RES_Subtotal_Partidas__c &lt; AB_DIV_Importe_a_ingresar__c, 
AB_LKP_Orden_de_Trabajo__r.OTR_RES_Facturar_Cliente_AI__c &lt; AB_DIV_Importe_a_ingresar__c), 
IF(OR(AB_LKP_Ingresar_a__r.CUE_FOR_RTDevName__c == &apos;Account:CUE_Compania&apos;,AB_LKP_Ingresar_a__r.CUE_FOR_RTDevName__c == &apos;Account:CUE_Cliente_B2C_ppal&apos;,AB_LKP_Ingresar_a__r.CUE_FOR_RTDevName__c == &apos;Account:CUE_Cliente_B2B_B2B2C_ppal&apos;), 
AB_LKP_Caso__r.CAS_DIV_Total_Facturar_CIA_AI__c &lt; AB_DIV_Importe_a_ingresar__c, 
AB_LKP_Caso__r.CAS_DIV_Total_Facturar_Cliente_AI__c &lt; AB_DIV_Importe_a_ingresar__c))</errorConditionFormula>
        <errorMessage>El importe a ingresar debe ser menor o igual que el importe facturado a CIA/Cliente</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_se_puede_modificar_abono_liquidado</fullName>
        <active>true</active>
        <errorConditionFormula>AND
   (
    NOT
	   (
	    ISCHANGED(AB_CAS_Abono_Liquidado__c)
		),
	AB_CAS_Abono_Liquidado__c,
    NOT(ISNEW()) 
	)</errorConditionFormula>
        <errorMessage>No se pueden modificar abonos que ya estén liquidados</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Relacion_imputar_importe</fullName>
        <active>true</active>
        <description>Revisa si un &quot;imputar a:&quot; tiene su importe asociado</description>
        <errorConditionFormula>OR(
	AND(
		AB_DIV_Importe_a_Imputar4__c = 0,
                !ISBLANK(AB_LKP_Imputar_a4__c)
		),
	AND(AB_DIV_Importe_a_Imputar4__c &gt; 0,
		ISBLANK(AB_LKP_Imputar_a4__c)
		),
	AND(AB_DIV_Importe_a_Imputar5__c = 0,
		!ISBLANK(AB_LKP_Imputar_a5__c)
		),
	AND(AB_DIV_Importe_a_Imputar5__c &gt; 0,
		ISBLANK(AB_LKP_Imputar_a5__c)
		),
	AND(AB_DIV_Importe_a_Imputar6__c = 0,
		!ISBLANK(AB_LKP_Imputar_a6__c)
		),
	AND(AB_DIV_Importe_a_Imputar6__c &gt; 0,
		ISBLANK(AB_LKP_Imputar_a6__c)
		),
	AND(AB_DIV_Importe_a_Imputar__c = 0,
		!ISBLANK(AB_LKP_Imputar_a__c)
		),
	AND(AB_DIV_Importe_a_Imputar__c &gt; 0,
		ISBLANK(AB_LKP_Imputar_a__c)
		),
	AND(AB_DIV_Importe_a_Imputar2__c = 0,
		!ISBLANK(AB_LKP_Imputar_a2__c)
		),
	AND(AB_DIV_Importe_a_Imputar2__c &gt; 0,
		ISBLANK(AB_LKP_Imputar_a2__c)
		),
	AND(AB_DIV_Importe_a_Imputar3__c = 0,
		!ISBLANK(AB_LKP_Imputar_a3__c)
		),
	AND(AB_DIV_Importe_a_Imputar3__c &gt; 0,
		ISBLANK(AB_LKP_Imputar_a3__c)
		)
	)</errorConditionFormula>
        <errorMessage>Debe rellenarse tanto el profesional/gestor como el importe a imputar</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rellenar_Imputar_a</fullName>
        <active>true</active>
        <description>Para abonos generados de forma no automático, se debe rellenar al menos un lkp de los denominados imputar a, ya que debe haber al menos un evento asociado a dicho abono</description>
        <errorConditionFormula>AND(
	AB_CAS_Abono_Automatico__c = false, 
	ISBLANK(AB_LKP_Imputar_a__c), 
	ISBLANK(AB_LKP_Imputar_a2__c),
	ISBLANK(AB_LKP_Imputar_a3__c),
	ISBLANK(AB_LKP_Imputar_a4__c),
	ISBLANK(AB_LKP_Imputar_a5__c),
	ISBLANK(AB_LKP_Imputar_a6__c),
        ISBLANK(AB_DIV_Importe_a_imputar_Departamento__c)
	)</errorConditionFormula>
        <errorMessage>Se debe imputar al menos a un profesional o gestor</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Rellenar_Observaciones_Abono_Rechazado</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(AB_SEL_Estado__c,&quot;03&quot;)&amp;&amp;(AB_TXT_Observaciones__c = &apos;&apos;)</errorConditionFormula>
        <errorMessage>Si se rechaza la propuesta de Abono hay que indicar el motivo en el campo &apos;Observaciones&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Suma_importes_gestor</fullName>
        <active>true</active>
        <description>La suma de importes a gestores debe ser menor o igual al campo del servicio/intervención asociado</description>
        <errorConditionFormula>IF(NOT(ISBLANK(AB_LKP_Orden_de_Trabajo__c)), 
IF(OR(AB_LKP_Ingresar_a__r.CUE_FOR_RTDevName__c == &apos;Account:CUE_Compania&apos;,AB_LKP_Ingresar_a__r.CUE_FOR_RTDevName__c == &apos;Account:CUE_Cliente_B2C_ppal&apos;,AB_LKP_Ingresar_a__r.CUE_FOR_RTDevName__c == &apos;Account:CUE_Cliente_B2B_B2B2C_ppal&apos;), 
AB_LKP_Orden_de_Trabajo__r.OTR_RES_Subtotal_Partidas__c &lt; ( AB_DIV_Importe_a_Imputar__c + AB_DIV_Importe_a_Imputar2__c +AB_DIV_Importe_a_Imputar3__c+AB_DIV_Importe_a_imputar_Departamento__c), 
AB_LKP_Orden_de_Trabajo__r.OTR_RES_Facturar_Cliente_AI__c &lt; ( AB_DIV_Importe_a_Imputar__c + AB_DIV_Importe_a_Imputar2__c +AB_DIV_Importe_a_Imputar3__c+AB_DIV_Importe_a_imputar_Departamento__c)), 
IF(OR(AB_LKP_Ingresar_a__r.CUE_FOR_RTDevName__c == &apos;Account:CUE_Compania&apos;,AB_LKP_Ingresar_a__r.CUE_FOR_RTDevName__c == &apos;Account:CUE_Cliente_B2C_ppal&apos;,AB_LKP_Ingresar_a__r.CUE_FOR_RTDevName__c == &apos;Account:CUE_Cliente_B2B_B2B2C_ppal&apos;), 
AB_LKP_Caso__r.CAS_DIV_Total_Facturar_CIA_AI__c &lt; ( AB_DIV_Importe_a_Imputar__c + AB_DIV_Importe_a_Imputar2__c +AB_DIV_Importe_a_Imputar3__c+AB_DIV_Importe_a_imputar_Departamento__c), 
AB_LKP_Caso__r.CAS_DIV_Total_Facturar_Cliente_AI__c &lt; ( AB_DIV_Importe_a_Imputar__c + AB_DIV_Importe_a_Imputar2__c +AB_DIV_Importe_a_Imputar3__c+AB_DIV_Importe_a_imputar_Departamento__c)))</errorConditionFormula>
        <errorMessage>El importe imputado a gestores debe ser menor o igual al importe total facturado a CIA/Cliente</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Suma_importes_profesional</fullName>
        <active>true</active>
        <description>La suma de importes a profesional debe ser menor o igual al campo del servicio/intervención asociado</description>
        <errorConditionFormula>IF(NOT(ISBLANK(AB_LKP_Orden_de_Trabajo__c)),AB_LKP_Orden_de_Trabajo__r.OTR_RES_Liquidar_prof_AI__c &lt; ( AB_DIV_Importe_a_Imputar4__c + AB_DIV_Importe_a_Imputar5__c +AB_DIV_Importe_a_Imputar6__c), AB_LKP_Caso__r.CAS_DIV_Total_Facturar_Profesional__c &lt; ( AB_DIV_Importe_a_Imputar4__c + AB_DIV_Importe_a_Imputar5__c +AB_DIV_Importe_a_Imputar6__c))</errorConditionFormula>
        <errorMessage>El importe imputado a profesionales debe ser menor o igual al importe liquidado al profesional</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>control_sobrepaso_importes</fullName>
        <active>false</active>
        <description>Controla que los importes de facturación no sobrepasen el total del importe a ingresar</description>
        <errorConditionFormula>AB_DIV_Importe_a_ingresar__c   &lt;&gt;  (AB_DIV_Importe_a_Imputar__c+AB_DIV_Importe_a_Imputar2__c+AB_DIV_Importe_a_Imputar3__c+AB_DIV_Importe_a_Imputar4__c+AB_DIV_Importe_a_Imputar5__c+AB_DIV_Importe_a_Imputar6__c)</errorConditionFormula>
        <errorMessage>La suma de importes facturados debe ser igual el importe total a ingresar</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>importe_menor_servicio_asociado</fullName>
        <active>true</active>
        <description>el importe introducido es menor o igual al campo del servicio asociado</description>
        <errorConditionFormula>IF(OR(AB_LKP_Ingresar_a__r.RecordType.DeveloperName = &quot;CUE_Compania&quot;, AB_LKP_Ingresar_a__r.RecordType.DeveloperName = &quot;CUE_Cliente_B2B_B2B2C_ppal&quot;,AB_LKP_Ingresar_a__r.RecordType.DeveloperName = &quot;CUE_Cliente_B2C_ppal&quot;),  AB_LKP_Orden_de_Trabajo__r.OTR_RES_Subtotal_Partidas__c &gt;  AB_LKP_Caso__r.CAS_DIV_Total_Facturar_CIA_AI__c,  AB_LKP_Orden_de_Trabajo__r.OTR_RES_Facturar_Cliente_AI__c &gt;  AB_LKP_Caso__r.CAS_DIV_Total_Facturar_Cliente_AI__c )</errorConditionFormula>
        <errorMessage>el importe introducido es menor o igual al campo del servicio asociado</errorMessage>
    </validationRules>
